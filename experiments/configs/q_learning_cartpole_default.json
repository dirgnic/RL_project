{
    "experiment_name": "q_learning_cartpole_default",
    "seed": 42,
    "environment": {
        "name": "CartPole-v1",
        "reward_type": "default"
    },
    "hyperparameters": {
        "alpha": 0.1,
        "gamma": 0.99,
        "epsilon_start": 1.0,
        "epsilon_end": 0.01,
        "epsilon_decay": 0.995,
        "num_episodes": 1000
    },
    "discretization": {
        "enabled": true,
        "bins": {
            "0": 10,
            "1": 10,
            "2": 10,
            "3": 10
        }
    },
    "logging": {
        "log_interval": 50,
        "save_model": true,
        "save_interval": 200
    }
}
