{
    "experiment_name": "q_learning_lunarlander_fuel_efficiency",
    "seed": 42,
    "environment": {
        "name": "LunarLander-v2",
        "reward_type": "fuel_efficiency"
    },
    "hyperparameters": {
        "alpha": 0.1,
        "gamma": 0.99,
        "epsilon_start": 1.0,
        "epsilon_end": 0.01,
        "epsilon_decay": 0.995,
        "num_episodes": 2000
    },
    "discretization": {
        "enabled": true,
        "bins": {
            "0": 10,
            "1": 10,
            "2": 10,
            "3": 10,
            "4": 10,
            "5": 10,
            "6": 2,
            "7": 2
        }
    },
    "logging": {
        "log_interval": 100,
        "save_model": true,
        "save_interval": 400
    }
}
